{
  "RateLimit": {
    "Enabled": true,
    "LimitPeriodDays": 30,
    "LimitApiRequestsCount": 15,
    "Notification": {
      "Enabled": true,
      "LimitPercentageForNotification": 82
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
